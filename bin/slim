declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

use Slim\Console\Application;
use Slim\Console\Command\CommandRegistry;

$app = new Application();

// Register all the commands in the src/Command directory
CommandRegistry::register($app);
$app->run();
